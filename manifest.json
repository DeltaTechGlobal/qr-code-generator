{
  "manifest_version": 3,
  "name": "QR Code Generator",
  "version": "1.0.0",
  "description": "Generate custom QR codes for various purposes including vCards, WiFi, URLs, and more",
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": ["storage"],
  "author": "your@email.com"
} 